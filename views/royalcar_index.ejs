    <!-- Rent A Car Start -->
    <div class="container-fluid bg-light py-6 px-5" id="rentacar">
        <div class="text-center mx-auto mb-5" style="max-width: 800px;">
            <h1 class="display-1 text-primary">03</h1>
            <h1 class="display-5 text-uppercase mb-4">Find Your <span class="text-primary">Car</span></h1>
        </div>
        <div class="row g-5">
            <% const cars = [
                { img: "/img/royalcar/car-rent-1.png", name: "Mercedes Benz R3", year: "2015", trans: "AUTO", mileage: "25K", price: "₹3500.00/Day" },
                { img: "/img/royalcar/car-rent-2.png", name: "Mercedes Benz R3", year: "2015", trans: "AUTO", mileage: "25K", price: "$99.00/Day" },
                { img: "/img/royalcar/car-rent-3.png", name: "Mercedes Benz R3", year: "2015", trans: "AUTO", mileage: "25K", price: "$99.00/Day" },
                { img: "/img/royalcar/car-rent-4.png", name: "Mercedes Benz R3", year: "2015", trans: "AUTO", mileage: "25K", price: "$99.00/Day" },
                { img: "/img/royalcar/car-rent-5.png", name: "Mercedes Benz R3", year: "2015", trans: "AUTO", mileage: "25K", price: "$99.00/Day" },
                { img: "/img/royalcar/car-rent-6.png", name: "Mercedes Benz R3", year: "2015", trans: "AUTO", mileage: "25K", price: "$99.00/Day" }
            ]; %>
            <% cars.forEach((car, index) => { %>
            <div class="col-lg-4 col-md-6">
                <div class="bg-white text-center border border-light rounded p-4 h-100 shadow-sm">
                    <img class="img-fluid mb-3" src="<%= car.img %>" alt="<%= car.name %>">
                    <h4 class="text-uppercase mb-3"><%= car.name %></h4>
                    <div class="d-flex justify-content-center mb-3 text-muted">
                        <div class="mx-2"><i class="fa fa-car text-primary me-1"></i> <%= car.year %></div>
                        <div class="mx-2 border-start border-end px-3"><i class="fa fa-cogs text-primary me-1"></i> <%= car.trans %></div>
                        <div class="mx-2"><i class="fa fa-road text-primary me-1"></i> <%= car.mileage %></div>
                    </div>
                    <a class="btn btn-primary px-4" href="#"> <%= car.price %> </a>
                </div>
            </div>
            <% }); %>
        </div>
    </div>
    <!-- Rent A Car End -->

    <!-- Banner Start -->
    <div class="container-fluid py-6 px-5">
        <div class="row g-4">
            <!-- Left Banner -->
            <div class="col-lg-6">
                <div class="bg-secondary d-flex align-items-center justify-content-between p-4 rounded h-100">
                    <img src="/img/royalcar/banner-left.png" class="img-fluid w-50 me-4 d-none d-md-block" alt="Become a driver">
                    <div class="text-light text-end">
                        <h3 class="text-uppercase mb-3">Want to be driver?</h3>
                        <p class="mb-4">Ready to hit the road? Join us and become a skilled driver with our professional training programs, designed to equip you with the confidence and expertise for a safe and successful driving journey.</p>
                        <a class="btn btn-primary py-2 px-4" href="#contact">Contact Now</a>
                    </div>
                </div>
            </div>

            <!-- Right Banner -->
            <div class="col-lg-6">
                <div class="bg-dark d-flex align-items-center justify-content-between p-4 rounded h-100">
                    <div class="text-light">
                        <h3 class="text-uppercase mb-3">Looking for a car?</h3>
                        <p class="mb-4">Find your perfect ride! Explore our wide selection of quality cars, tailored to meet your needs and preferences, ensuring a seamless and enjoyable driving experience.</p>
                        <a class="btn btn-primary py-2 px-4" href="#service">Start Now</a>
                    </div>
                    <img src="/img/royalcar/banner-right.png" class="img-fluid w-50 ms-4 d-none d-md-block" alt="Looking for a car">
                </div>
            </div>
        </div>
    </div>
